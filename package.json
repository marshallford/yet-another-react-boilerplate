{
  "name": "yet-another-react-boilerplate",
  "version": "1.0.0",
  "description": "yet another react boilerplate",
  "main": "./src/js/main.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "build:dev": {
      "command": "npm run clean && webpack --env.dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "npm run clean &&  webpack --env.prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "nodemon --exec webpack-dev-server --env.dev",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marshallford/yet-another-react-boilerplate.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marshallford/yet-another-react-boilerplate/issues"
  },
  "homepage": "https://github.com/marshallford/yet-another-react-boilerplate#readme",
  "dependencies": {
    "axios": "^0.13.1",
    "immutable": "^3.8.1",
    "lodash": "^4.15.0",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.11",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0-beta.3",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.10.1",
    "redbox-react": "^1.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-server": "^2.1.0-beta.0"
  }
}
