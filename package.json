{
  "name": "yet-another-react-boilerplate",
  "version": "1.0.0",
  "description": "yet another react boilerplate",
  "main": "./src/js/main.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "build:dev": {
      "command": "npm run clean && webpack --env.dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "npm run clean &&  webpack --env.prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "nodemon --exec webpack-dev-server --env.dev",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marshallford/yet-another-react-boilerplate.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marshallford/yet-another-react-boilerplate/issues"
  },
  "homepage": "https://github.com/marshallford/yet-another-react-boilerplate#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "lodash": "^4.17.4",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router-dom": "^4.0.0-beta.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.4",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.3.1",
    "redbox-react": "^1.3.4",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
