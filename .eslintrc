{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react",
  ],
  "plugins": [
    "babel",
    "react",
    "promise",
  ],
  "env": {
    "browser": true,
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "jsx-quotes": ["error", "prefer-single"],
  }
}
